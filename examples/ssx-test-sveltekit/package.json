{
  "name": "ssx-test-sveltekit",
  "version": "0.1.0",
  "private": true,
  "description": "An example project for SSX with SvelteKit",
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.30.4",
    "svelte": "^3.59.2",
    "svelte-check": "^2.10.3",
    "typescript": "^4.9.5",
    "vite": "^4.5.2"
  },
  "dependencies": {
    "@spruceid/ssx": "*",
    "@spruceid/ssx-server": "*",
    "siwe": "^2.1.4"
  },
  "type": "module"
}
