{
  "name": "@spruceid/ssx-authjs",
  "version": "1.1.0",
  "description": "SSX AuthJS library",
  "author": "Spruce Systems, Inc.",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/spruceid/ssx/packages/ssx-authjs#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/spruceid/ssx/issues"
  },
  "directories": {
    "src": "src"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.js",
      "default": "./dist/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.js",
      "default": "./dist/server/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "server": [
        "dist/server/index.d.ts"
      ],
      "client": [
        "dist/client/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:spruceid/ssx.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --jsx react-jsx"
  },
  "dependencies": {
    "@auth/core": "^0.4.0",
    "@spruceid/ssx": "^1.2.5",
    "@spruceid/ssx-server": "1.2.4",
    "@sveltejs/kit": "^1.8.8",
    "next-auth": "^4.10.3",
    "siwe": "^2.1.3"
  },
  "devDependencies": {
    "@auth/sveltekit": "^0.2.1",
    "@types/react": "^18.0.21",
    "next": "^12.1.6",
    "react": "^18.2.0",
    "typescript": "^4.8.4",
    "vite": "^4.1.4"
  },
  "files": [
    "tsconfig.json",
    "dist",
    "next"
  ]
}
